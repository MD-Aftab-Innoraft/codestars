<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}CodeClub
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">

		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
			<link rel="stylesheet" href="{{ asset('css/style.css') }}">
		{% endblock %}


	</head>

	<body>
<div>
	{# Iterate over each flash message and display it #}
	{% for label, messages in app.flashes %}
		{% for message in messages %}
			<div class="flash-msg flash-{{ label }}">
				{{ message }}
			</div>
		{% endfor %}
	{% endfor %}
</div>

{% block navbar %}
	<div class="user-navbar">
		<div class="user-navbar-logo">
			<a href="/dashboard">CodeClub</a>
		</div>
		<ul class="user-nav-links">
			<li>
				<a href="/dashboard">Home</a>
			</li>
			<li>
				<a href="/user-profile">Profile</a>
			</li>
			<li>
				<a href="/upcoming-exams">Upcoming Exams</a>
			</li>
			<li>
				<a href="/applied-exams">Applied exams</a>

			</li>
			<li>
				<a href="/logout">Logout</a>
			</li>
		</ul>
	</div>

{% endblock %}

		<div class="container"> {% block body %}{% endblock %}
			</div>
		</body>
{% block javascripts %}
	{{ encore_entry_script_tags('app') }}
	<script src="{{ asset('JS/script.js') }}"></script>
{% endblock %}

	</html>

